{
  "name": "dp-frontend-new",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.1",
    "antd": "^3.20.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "axios": "^0.21.1",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "graphql-tag": "^2.10.1",
    "graphql.macro": "^1.4.2",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "store": "^2.0.12",
    "typescript": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "ts:verify": "tsc --noEmit",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prettier": "prettier --write ./**/*.{js,ts,jx,tsx,css,json,md,graphql} --ignore-path ./.prettierignore",
    "precommit:prettier": "pretty-quick --staged",
    "precommit:test": "cross-env CI=true react-scripts test --bail --lastCommit --noStackTrace --silent --colors",
    "eject": "react-scripts eject",
    "watch": "watch \"react-scripts build\" ./src",
    "serve": "serve -l 3000 -s build",
    "apollo:schema": "apollo schema:download ./src/model/graphql/schema.json --endpoint=http://localhost:8080/graphql",
    "apollo:codegen": "apollo codegen:generate --target=typescript --addTypename --globalTypesFile=./src/model/__generated__/globalTypes.ts"
  },
  "homepage": "/new",
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit:prettier"
    }
  },
  "setupFiles": [
    "./src/setupTests.ts"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chart.js": "^2.7.54",
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/graphql": "^14.2.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.123",
    "@types/node": "11.13.0",
    "@types/react": "16.8.12",
    "@types/react-dom": "16.8.3",
    "@types/react-icons": "^3.0.0",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-router-redux": "^5.0.18",
    "@types/react-test-renderer": "^16.8.2",
    "@types/recompose": "^0.30.6",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.1",
    "@types/store": "^2.0.1",
    "apollo": "^2.13.1",
    "apollo-link-schema": "^1.2.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "graphql-tools": "^4.0.5",
    "husky": "^1.3.1",
    "mock-apollo-client": "^0.1.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.8.6",
    "recompose": "^0.30.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "serve": "^11.0.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "watch": "^1.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/dashboard/**/**/*.{js,jsx,ts,tsx}",
      "src/components/applications/*.{js,jsx,ts,tsx}",
      "src/components/teams/**/**/*.{js,jsx,ts,tsx}",
      "src/components/createTeam/*.{js,jsx,ts,tsx}",
      "src/components/common/**/**/*.{js,jsx,ts,tsx}",
      "src/components/routes/**/**/*.{js,jsx,ts,tsx}",
      "src/utils/*.{js,jsx,ts,tsx}"
    ]
  }
}
